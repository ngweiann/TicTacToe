{
    "name": "TicTacToe",
	"short_name": "TicTac",
    "manifest_version": 2,
	"version":"1.0.0.0",
    "description": "This is 3x3 tic tac toe game. Have fun!",
    "browser_action": {
		"default_icon": "48.png",
		"default_popup": "popup.html",
		"default_title": "Launch Game"
	},
	"background":{
		"scripts":["background.js"],
		"persistent": false
	},
	"permissions": [
        "tabs",
        "activeTab",
        "storage",
        "<all_urls>"
    ],
	"icons": {"48": "48.png",
			"16": "16.png",
			"128": "128.png"}
}